{
  "name": "eonext_account",
  "version": "1.0.0",
  "description": "eonext account frontend",
  "author": "chato",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development NUXT_HOST=0.0.0.0 NUXT_PORT=4000 nodemon server/index.js --watch server",
    "build": "nuxt build",
